
<div class = "jumbotron header">
  <h1>Contact Book</h1>
</div>

<form action="/success" method="post">
  <label for="firstName"></label>
  <input id="firstName" name="firstName" type="text" placeholder = "First Name">
  <label for="lastName"></label>
  <input id="lastName" name="lastName" type="text" placeholder="Last Name">
  <label for="jobTitle"></label>
  <input id="jobTitle" name="jobTitle" type="text" placeholder = "Job Title">
  <label for="company"></label>
  <input id="company" name="company" type="text" placeholder = "Company">
  <button type = "submit" class = "btn btn-success">Add this Contact</button>
</form>

<a href="/contacts" id="contactLink">View your contacts</a>
